/div>\n</template>\n<script>\nimport {mapState, mapMutations} from \"vuex\";\n\nexport default {\n  computed: {\n    ...mapState({\n      tags: state => state.tab.tabsList\n    })\n  },\n  data() {\n    return {\n      dynamicTags: ['标签一', '标签二', '标签三'],\n      inputVisible: false,\n      inputValue: ''\n    };\n  },\n  methods: {\n    ...mapMutations({\n      close: 'closeTab'\n    }),\n    handleClose(tag) {\n      this.close(tag)\n      this.$router.push({name:'home'})\n    },\n    changeMenu(item) {\n      this.$router.push({name: item.name})\n      this.$store.commit('selectMenu', item)\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.tabs {\n  padding: 20px;\n\n  .el-tag {\n    margin-right: 15px;\n    cursor: pointer;\n  }\n}\n</style>"]}]}